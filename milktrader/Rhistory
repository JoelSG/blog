require(milk)
gas = as.xts(read.zoo('~/Desktop/gasolineGulfCoast.csv', format='%d-%b-%Y', sep=',', skip=6))
gas = gas[,1]
gas$RET =dailyReturn(gas)
g = apply.monthly(gas, last)
png('gasoline.png')
zeroplot(g$RET, up.col='maroon1', dn.col='maroon4', main='Gasoline\n (Gulf Coast spot)', xlab='1986 to 2011')
