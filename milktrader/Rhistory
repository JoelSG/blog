require(milk)
clear()
data(opex)
getSymbols('^GSPC' , from=index(first(opex)))
GSPC$RET = dailyReturn(Cl(GSPC))
G = GSPC[index(opex)]
g = G[.indexmon(G) == 6]
nrow(g)
around_zero(g$RET)
around_zero(g$RET, xlab='2000 to 2011', main='SPX returns on OpEx Friday \n in July')
png('opex_friday_in_july.png')
around_zero(g$RET, xlab='2000 to 2011', main='SPX returns on OpEx Friday \n in July')
dev.off()
dev.off()
savehistory()
